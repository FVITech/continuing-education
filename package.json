{
    "name": "continuing-education",
    "version": "1.0.0",
    "description": "Continuing education landing page for FVI",
    "main": "src/js/index.js",
    "license": "MIT",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --env=dev --config ./webpack/index.babel.js",
        "prod": "rimraf dist && cross-env webpack -p --env=prod --progress --profile --colors --config ./webpack/index.babel.js && git add -A && git commit -m \"build\"",
        "dgh": "git push origin --delete gh-pages",
        "gh-pages": "git subtree push --prefix dist origin gh-pages && rimraf dist"
    },
    "devDependencies": {
        "autoprefixer": "^6.7.7",
        "babel-core": "^6.24.0",
        "babel-loader": "^6.4.1",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-preset-env": "^1.3.2",
        "chunk-manifest-webpack-plugin": "^1.0.0",
        "copy-webpack-plugin": "^4.0.1",
        "cross-env": "^4.0.0",
        "css-loader": "^0.28.0",
        "extract-text-webpack-plugin": "^2.1.0",
        "favicons-webpack-plugin": "^0.0.7",
        "file-loader": "^0.11.1",
        "html-webpack-plugin": "^2.28.0",
        "image-webpack-loader": "^3.3.0",
        "node-sass": "^4.5.2",
        "postcss-loader": "^1.3.3",
        "pug": "^2.0.0-beta11",
        "pug-loader": "^2.3.0",
        "resource-hints-webpack-plugin": "^0.0.1",
        "rimraf": "^2.6.1",
        "sass-loader": "^6.0.3",
        "style-loader": "^0.16.1",
        "url-loader": "^0.5.8",
        "webpack-chunk-hash": "^0.4.0",
        "webpack-dev-server": "^2.4.2"
    },
    "dependencies": {
        "jquery": "^3.2.1",
        "lodash.throttle": "^4.1.1",
        "webpack": "^2.3.2",
        "webpack-dev-middleware": "^1.10.1"
    },
    "browserslist": [
        "last 8 versions"
    ]
}
